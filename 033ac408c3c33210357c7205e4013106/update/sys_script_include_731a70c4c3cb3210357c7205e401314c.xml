        <script><![CDATA[var CasitaUtils = Class.create();
CasitaUtils.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

    grabAvailableFunds: function() {
        var funds = '';
        var parent = this.getParameter('sysparm_parent');
		var subparent = this.getParameter('sysparm_sub_parent');
		if(parent){
        var gt = new GlideRecord('u_casita_monthly_project');
        gt.get(parent);
        funds = gt.u_available_funds;
		} else{
			var gp = new GlideRecord('u_sub_casita_monthly_project');
			gp.get(subparent);
			funds = gp.u_parent_project.u_available_funds;
		}
        return funds;
    },

    type: 'CasitaUtils'
});]]></script>
